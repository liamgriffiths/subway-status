{
  "name": "react-es6-subways",
  "version": "1.0.0",
  "description": "ooh aah",
  "main": "index.js",
  "scripts": {
    "start": "npm run build-client && npm run start-server",
    "build-client": "mkdir -p static && browserify client/client.js -o static/bundle.js --verbose",
    "watch-client": "mkdir -p static && watchify client/client.js -o static/bundle.js --verbose",
    "start-server": "babel-node server/server",
    "watch-server": "nodemon --exec babel-node server/server"
  },
  "keywords": [
    "mta",
    "subways",
    "es6",
    "react",
    "demo"
  ],
  "author": "liamgriffiths <liamgriffiths@gmail.com>",
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "browserify": "^10.2.4"
  },
  "dependencies": {
    "babel": "^5.5.4",
    "bluebird": "^2.9.27",
    "envify": "^3.4.0",
    "express": "^4.12.4",
    "react": "^0.13.3",
    "react-style": "^0.5.5",
    "superagent": "^1.2.0",
    "xml-parser": "^1.2.1"
  }
}
