{
  "name": "subway-statuses",
  "version": "1.0.0",
  "description": "i just wanted to use all the nice things",
  "repository": "git@github.com:liamgriffiths/subway-status.git",
  "scripts": {
    "test": "mocha --reporter spec --compilers js:babel/register test/*",
    "start": "npm run build-client && npm run start-server",
    "build-client": "browserify client/client.js -o static/bundle.js",
    "watch-client": "watchify client/client.js -o static/bundle.js",
    "start-server": "babel-node server/server",
    "watch-server": "nodemon --exec babel-node server/server"
  },
  "keywords": [
    "mta",
    "subways",
    "g train",
    "es6",
    "es7",
    "react",
    "demo"
  ],
  "author": "liamgriffiths <liamgriffiths@gmail.com>",
  "license": "MIT",
  "browserify": {
    "verbose": true,
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "dependencies": {
    "babel": "5.5.4",
    "babelify": "6.1.2",
    "bluebird": "2.9.27",
    "browserify": "10.2.4",
    "envify": "3.4.0",
    "express": "4.12.4",
    "react": "0.13.3",
    "react-style": "0.5.5",
    "superagent": "1.2.0",
    "uglifyify": "3.0.1",
    "xml-parser": "1.2.1"
  },
  "devDependencies": {
    "mocha": "^2.2.5"
  }
}
